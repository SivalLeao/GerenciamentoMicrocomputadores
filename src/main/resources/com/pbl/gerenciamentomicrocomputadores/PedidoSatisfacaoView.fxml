<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="279.0" prefWidth="362.0" styleClass="cadastro" stylesheets="@CSS/style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pbl.gerenciamentomicrocomputadores.controller.PedidoSatisfacaoController">
   <children>
      <Label layoutX="85.0" layoutY="14.0" prefHeight="38.0" prefWidth="193.0" text="Satisfação do cliente" textFill="WHITE">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="satisfacaoTexto" alignment="TOP_LEFT" layoutX="60.0" layoutY="63.0" prefHeight="106.0" prefWidth="242.0" promptText="Digite a satisfação do cliente." />
      <Button fx:id="responderBotao" layoutX="146.0" layoutY="210.0" mnemonicParsing="false" onAction="#responderAcao" text="responder" />
      <Label fx:id="mensagemDeErroSatisfacao" layoutX="39.0" layoutY="169.0" text="Entrada inválida, digite apenas caracteres alfabeticos ." textFill="#e50000" />
   </children>
</AnchorPane>
